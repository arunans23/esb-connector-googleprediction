<component name="googleprediction-trainedmodels" type="synapse/template">
  <subComponents>
    <component name="getAnalyzeTrainedModel">
      <file>getAnalyzeTrainedModel.xml</file>
      <description>Get analysis of the model and the data the model was trained on.</description>
    </component>
    <component name="deleteTrainedModel">
      <file>deleteTrainedModel.xml</file>
      <description>Delete a trained model.</description>
    </component>
    <component name="insertTrainedModel">
      <file>insertTrainedModel.xml</file>
      <description>Train a Prediction API model.</description>
    </component>
    <component name="listTrainedModels">
      <file>listTrainedModels.xml</file>
      <description>List available models.</description>
    </component>
    <component name="predictTrainedModel">
      <file>predictTrainedModel.xml</file>
      <description>Submit model id and request a prediction.</description>
    </component>
    <component name="updateTrainedModel">
      <file>updateTrainedModel.xml</file>
      <description>Add new data to a trained model</description>
    </component>
  </subComponents>
</component>